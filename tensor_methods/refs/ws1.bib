  shorttitle = {Fermi {{Surface}} of \$\{\textbackslash mathrm\{\vphantom{\}\}}{{Sr}}\vphantom\{\}\vphantom\{\}\_\{2\}\{\textbackslash mathrm\{\vphantom{\}\}}{{RuO}}\vphantom\{\}\vphantom\{\}\_\{4\}\$},
