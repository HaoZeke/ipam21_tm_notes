  file = {/Users/rohitgoswami/Zotero/storage/38FNY8TX/Georges et al_1996_Dynamical mean-field theory of strongly correlated fermion systems and the.pdf;/Users/rohitgoswami/Zotero/storage/8YBAK5GR/RevModPhys.68.html},
